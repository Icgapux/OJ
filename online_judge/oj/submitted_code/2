#include <bits/stdc++.h>
using namespace std;
int Add(int a,int b);
int main() {
    int x,y,sum;
    cout << "Please input x and y :" << '\n';
    cin >> x >> y;
    sum = Add(x,y);
    std::cout << x << " + "<< y << " = " << sum  << '\n';
    return 0;
}
int Add(int a,int b){
    return a + b;
}
